{
	"name": "kdyby/monolog",
	"type": "library",
	"description": "Integration of Monolog into Nette Framework",
	"keywords": ["nette", "monolog", "logging", "diagnostics"],
	"homepage": "http://kdyby.org",
	"license": ["BSD-3-Clause", "GPL-2.0", "GPL-3.0"],
	"authors": [
		{
			"name": "Martin Bažík",
			"email": "martin@bazo.sk",
			"homepage": "http://bazo.sk"
		},
		{
			"name": "Filip Procházka",
			"homepage": "http://filip-prochazka.com",
			"email": "filip@prochazka.su"
		}
	],
	"require": {
		"php": "^7.1  || ^7.2  || ^7.3",
		"nette/di": "~3.0.0@dev",
		"kdyby/strict-objects": "~1.0.0",
		"monolog/monolog": "^1.24",
		"tracy/tracy": "~2.6 || ~3.0.0@dev",
		"nette/caching": "~2.5 || ~3.0.0@dev"
	},
	"require-dev": {
		"nette/application": "~3.0.0@dev",
		"nette/bootstrap": "~2.4 || ~3.0.0@dev",
		"nette/http": "~2.4 || ~3.0.0@dev",
		"nette/tester": "~2.2",
		"phpstan/phpstan-shim": "~0.11.4",
		"jakub-onderka/php-parallel-lint": "~1.0.0",
		"kdyby/coding-standard": "^2.0@dev"
	},
	"minimum-stability": "dev",
	"support": {
		"email": "filip@prochazka.su",
		"issues": "https://github.com/kdyby/monolog/issues"
	},
	"autoload": {
		"psr-4": {
			"Kdyby\\Monolog\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"KdybyTests\\": "tests/KdybyTests/"
		},
		"classmap": [
			"tests/KdybyTests"
		]
	},
	"provide": {
		"psr/log-implementation": "1.0.0"
	},
	"extra": {
		"branch-alias": {
			"dev-master": "1.3-dev"
		}
	}
}
